{
    "version": 2,
    "builds": [
        {
            "src": "main.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(upload|v/.*)",
            "dest": "/main.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}